{
  "extends": ["next/core-web-vitals", "plugin:import/recommended"],
  "plugins": ["unused-imports", "import"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "import/order": "off",
    "import/named": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
